{
      "name": "Character_Catcher",
      "description": "Character Husbando/waifus Catcher Group Bot.",
      "logo": "https://telegra.ph/file/14d693b36bd6f1307704a.jpg",
      "keywords": [
         "telegram",
         "modular",
         "group",
         "manager",
         "Character catcher",
         "waifu bot",
         "husbando"
         "harem bot"
      ],   
   "repository": "https://github.com/SleepyShuyaaa/character_catcher-",
   "stack": "heroku-22",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather",
         "required": true,
         "value": ""
      },
      
      "GROUP_ID": {
         "description": " leave it as it is.",
         "required": true,
         "value": ". /"
      },
      "SUDO_USER": {
         "description": "Set your best friends id as sudo.",
         "required": true,
         "value": "6149191605, 6765826972" 
      },
      "SUPPORT_CHAT": {
         "description": "your bots support chat name without @ or chat id.",
         "required": true,
         "value": "lmao" 
      },
      
      "OWNER_ID": {
         "description": "Id of bot owner",
         "required": true,
         "value": "" 
      },
     "MONGO_URL": {
         "description": "Add a Mongodb url to store data",
         "required": true,
         "value": "" 
      }
   }     
      
}
