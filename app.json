{
      "name": "Character_Catcher",
      "description": "Character Waifus Husbando Catcher Telegram Group Bot.",
      "logo": "https://telegra.ph/file/14d693b36bd6f1307704a.jpg",
      "keywords": [
         "telegram",
         "modular",
         "Waifu",
         "protecc",
         "character",
         "catcher",
         "waifu/husbando Catcher"
      ],   
   "repository": "https://github.com/SleepyShuyaaa/character_catcher-",
   "stack": "heroku-22",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather",
         "required": true,
         "value": ""
      },

      "SUDO_USERS": {
         "description": "Set your best friends id as sudo.",
         "required": true,
         "value": "6765826972 5798995982" 
      },
      "SUPPORT_CHAT": {
         "description": "your bots support chat name without @ or chat id.",
         "required": true,
         "value": "" 
      },
      "GROUP_ID": {
         "description": "Group/Channel Id whatever",
         "required": true,
         "value": "-123" 
      },
      "OWNER_ID": {
         "description": "Id of bot owner",
         "required": true,
         "value": "" 
      },
     "MONGO_URI": {
         "description": "Add a Mongodb url to store data",
         "required": true,
         "value": "" 
      }
   }     
      
}
